{
    "name": "gvital3230/laravel-notification-channel-viber-net-ua",
    "description": "viber.net.ua notification channel for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/viber-net-ua",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitalii Goncharov",
            "email": "1c.audit@gmail.com",
            "homepage": "https://github.com/gvital3230",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "6.* || ~7.0",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\ViberNetUa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ViberNetUa\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\ViberNetUa\\ViberNetUaServiceProvider"
            ]
        }
    }
}
